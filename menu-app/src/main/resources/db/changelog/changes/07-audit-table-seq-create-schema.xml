<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="
        http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.3.xsd">

   <changeSet id="create-audit-table-seq" author="habib-wahid">
       <createSequence sequenceName="revinfo_seq" startValue="1" incrementBy="1" />
   </changeSet>

    <changeSet id="create-revinfo-table" author="habib-wahid">
        <createTable tableName="revinfo">
            <column name="rev" type="bigint">
                <constraints primaryKey="true" nullable="false" />
            </column>
            <column name="revstmp" type="bigint" />
        </createTable>
    </changeSet>
</databaseChangeLog>
